.page{
  display: flex;
  flex-flow: column nowrap;
}

.page_bd{
  /* background-color:red; */
  flex: 1;
  overflow:scroll;
}




.page_ft{
  height: 104rpx;
  border-top:1px solid rgba(0,0,0,0.2);
}

.page_bar{
  height: 100%;
  display: flex;
  align-items: center;
}

.page_bar__index,.page_bar__dynamic,.page_bar__me{
  height:100%;
  flex: 1;
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
}

.page_bar__label{
  height:24rpx;
  font-size:9pt;
  font-family:serif;
  font-weight:300;
  color:#9f9f9f;
  line-height:24rpx;
}


.page_bar__index .page_bar__image{
background-image:url(data:image/png;base64,);
}

.page_bar__homefill{
  width:46rpx;
  height:46rpx;
  margin-top:14rpx;
  margin-bottom:10rpx;
  background-size: cover;
  background-image:url(data:image/png;base64,);
}

.page_bar__homefill + .page_bar__label{
  color:#31c2c2;
}

.page_bar__dynamic .page_bar__image{
  background-image:url(data:image/png;base64,);
}

.page_bar__dynamicfill{
  width:46rpx;
  height:46rpx;
  margin-top:14rpx;
  margin-bottom:10rpx;
  background-size: cover;
  background-image: url(data:image/png;base64,);
}

.page_bar__dynamicfill + .page_bar__label{
  color:#31c2c2;
}

.page_bar__me .page_bar__image{
  background-image:url(data:image/png;base64,);
}

.page_bar__myfill{
  width:46rpx;
  height:46rpx;
  margin-top:14rpx;
  margin-bottom:10rpx;
  background-size: cover;
  background-image: url(data:image/png;base64,);
}

.page_bar__myfill + .page_bar__label{
  color:#31c2c2;
}

.page_bar__image{
  width:46rpx;
  height:46rpx;
  margin-top:14rpx;
  margin-bottom:10rpx;
  background-size: cover;
}