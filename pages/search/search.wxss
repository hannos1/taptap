.page_bd{
  background-color:#f7f7f7;
  width:100vw;
  height:100vh;
}

.page_search{
  width:100%;
  overflow: hidden;
}

.search_search__inputmask,.page_search__searchlistitem{
  /* position: relative; */
  width:710rpx;
  height: 80rpx;
  margin: 12rpx 20rpx;
  /* padding:23rpx 19rpx; */
  text-align:left;
  /* box-sizing:border-box; */
  border-radius:10rpx;
  background-color: #ffffff;
  border:1rpx solid #efefef;
}

.search_search__input,.page_search__itembox{
  display: inline-block;
  width:630rpx;
  height:80rpx;
  margin-left:20rpx;
  font-size:10pt;
  line-height:32rpx;
}

.search_search__inputmask .label{
  font-size:10pt;
}

.cancelmask{
  display: inline-block;
  position: relative;
  height:100%;
  width: 50rpx;
}

.cancel{
  height:20rpx;
  width:20rpx;
  background-image: url('data:image/png;base64,');
  background-size: cover;
  background-position:-50%;
  display: inline-block;
  position: absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
}

.page_search__searchlist{
  width:100%;
  background-color: #ffffff;
  overflow: hidden;
}

.page_search__searchlistitem{
  border: none;
  position: relative;
}

.page_search__searchlistitem::before{
  content: '';
  position: absolute;
  left:8rpx;
  top:0;
  bottom:-12rpx;
  right:-20rpx;
  border-bottom:2rpx solid #f7f7f7;
}

.page_search__itembox{
  margin: 0;
  height:80rpx;
  width:650rpx;
}

.page_search__itemimgmask{
  width:60rpx;
  height:80rpx;
  position: relative;
  display: inline-block;
}

.page_search__itemimg{
  width: 32rpx;
  height:32rpx;
  background-size: cover;
  background-position: -50%;
  position: absolute;
  top:50%;
  left: 50%;
  transform:translate(-50%,-50%);
}

.page_search__itemlabel{
  display: inline-block;
  height:80rpx;
  /* background-color: red; */
  width:570rpx;
  margin-left:10rpx;
  overflow: hidden;
  font-size: 10pt;
  line-height:80rpx;
}

.page_hot{
  width:100%;
  margin-top: 18rpx;
  background-color: #ffffff;
  overflow: hidden;
}

.page_hot__title{
  display:inline-block;
  height:40rpx;
  margin-left:28rpx;
  margin-right:28rpx;
  margin-top: 30rpx;
  font-size:12pt;
  font-weight:700;
  text-align: left;
  line-height:40rpx;
}

.hot_bd{
  /* display: flex; */
  margin:14rpx 0;
  padding-left:28rpx;
  padding-right:28rpx;
  overflow: hidden;
}

.hot_bd__item{
  /* background-color: red; */
  width:347rpx;
  float: left;
  height:56rpx;
  display: flex;
}

.hot_bd__index{
  width:34rpx;
  height:56rpx;
  font-size:10pt;
  line-height: 56rpx;
  text-align:right;
  color: #21b0b3;
}

.hot_bd__context{
  margin-left:10rpx;
  flex:1;
  font-size:10pt;
  text-align:left;
  line-height:56rpx;
  height:56rpx;
}

.page_hottag{
  width:100%;
  margin-top: 18rpx;
  background-color: #ffffff;
  overflow: hidden;
}

.hottag_title{
  display:inline-block;
  height:40rpx;
  margin-left:28rpx;
  margin-right:28rpx;
  margin-top: 30rpx;
  font-size:12pt;
  font-weight:700;
  text-align: left;
  line-height:40rpx;
}

.hottag_taglist{
  margin:28rpx 0;
  padding-left:28rpx;
  padding-right:28rpx;
  overflow: hidden;
}

.hottag_item{
  /* display: inline-block; */
  padding:0 12rpx;
  height:48rpx;
  background-color: #f8f8f8;
  border:1rpx solid #e0dfdf;
  border-radius:4rpx;
  line-height:48rpx;
  font-size:10pt;
  text-align: center;
  color:#aeaeae;
  float: left;
  margin-right:24rpx;
  margin-bottom:24rpx;
}




